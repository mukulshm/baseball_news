<!-- START OF LIST -->
<!-- Start Addition of Panel -->
  <div class="col-sm-offset-1 col-sm-10 col-md-offset-2 col-md-8">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Twitter for  <%= @path.team.name %> with handle: <%= @twitter_path  %> </h3>
      </div>
      <div class="panel-body">
        <div class="row">
          <div class="col-md-12">
            <dl class="dl-horizontal">
              <ul class="list-group">
                <% @twitter.each do |twitter| %>
                  <li class="list-group-item">
                    <a href="<%=  twitter.url %>">
                      <%= twitter.text %>
                    </a>
                    <!-- with url of: <%=  twitter.url %> -->
                  </li>
                <% end %>
              </ul>
            </dl>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<!-- END OF LIST -->
